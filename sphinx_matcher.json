{
    "problemMatcher": [
        {
            "owner": "sphinx-problem-matcher",
            "pattern": [
                {
                    "regexp": "^(.*):(\\d*):\\s+(\\w*):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "severity": 3,
                    "message": 4
                }
            ]
        }
    ]
}